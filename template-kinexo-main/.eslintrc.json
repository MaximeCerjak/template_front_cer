{
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": ["javascript", "react"],
    "extends": ["eslint:recommended", "plugin:javascript/recommended", "plugin:react/recommended"],
    "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "single"],
      "arrow-parens": ["error", "always"],
      "brace-style": ["error", "1tbs", { "allowSingleLine": true }],
      "indent": ["error", 2],
      "no-unused-vars": "warn",
      "react/jsx-uses-vars": "warn",
      "react/prop-types": "off"
    }
  }
  